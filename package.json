{
  "name": "quicker.style",
  "version": "0.0.35",
  "private": false,
  "type": "module",
  "main": "./dist/quicker.style.umd.cjs",
  "module": "./dist/quicker.style.es.js",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "import": "./dist/quicker.style.es.js",
      "require": "./dist/quicker.style.umd.cjs",
      "types": "./dist/main.d.ts",
      "style": "./dist/style.css"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only types",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "build:preproduction": "prettier --write src/ && vitest run && vite build",
    "build:production": "npm run build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "types": "npx tsc --emitDeclarationOnly"
  },
  "peerDependencies": {
    "vue": "^3.4.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node18": "^18.2.2",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^18.19.3",
    "@vicons/fluent": "^0.12.0",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.3",
    "@vue/tsconfig": "^0.5.0",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "glob": "^10.3.10",
    "jsdom": "^23.0.1",
    "npm-run-all2": "^6.1.1",
    "prettier": "^3.0.3",
    "sass": "^1.69.7",
    "seemly": "^0.3.8",
    "typescript": "~5.3.0",
    "vite": "^5.0.10",
    "vite-plugin-dts": "^3.7.0",
    "vite-plugin-lib-inject-css": "^1.3.0",
    "vitest": "^1.0.4",
    "vue": "^3.4.5",
    "vue-tsc": "^1.8.25"
  },
  "sideEffects": [
    "**/*.css",
    "**/*.scss"
  ],
  "dependencies": {
    "@floating-ui/dom": "^1.6.12",
    "@heroicons/vue": "^2.2.0",
    "@vueuse/core": "^10.7.2",
    "composed-offset-position": "^0.0.6"
  }
}
